{
  "name": "makeup-switch",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "description": "A JavaScript class representing an ARIA switch",
  "version": "0.3.2",
  "repository": "https://github.com/makeup/makeup-js/tree/master/packages/ui/makeup-switch",
  "author": "Ian McBurnie <ianmcburnie@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "npm run compile:mjs && npm run compile:cjs",
    "compile:mjs": "esbuild src/index.js --outfile=dist/mjs/index.js --format=esm",
    "compile:cjs": "babel --plugins @babel/plugin-transform-modules-commonjs src/index.js --out-file dist/cjs/index.js"
  },
  "files": [
    "dist/cjs/index.js",
    "dist/mjs/index.js",
    "dist/mjs/package.json",
    "package-lock.json"
  ],
  "browserslist": [
    "extends @ebay/browserslist-config"
  ]
}
